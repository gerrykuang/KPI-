<view class="container"> 

  <view class="login-icon"> 
 
      <image class="im" src="../../images/login.png"></image>
   
       <view class="te1">WCQ Y2020 </view>
      <view class="te1">2H KPI Review Meeting</view>

      <view class="te2">逆風揚帆 劈波斬浪</view>

    <!-- <image class="login-img" src="/images/login.jpg"></image>  -->
   
  </view> 
  <view class="login-iii"></view>

  <view class="login-from"> 
 
    <!--账号-->
    <view class="inputView"> 
      <!-- <image class="nameImage" src="/images/qq.jpg"></image>  -->
      <label class="loginLab">帳號</label> 
      <input class="inputText" placeholder="請輸入工號 " bindinput="phoneInput" /> 
    </view> 
    <view class="line"></view> 
    <!--密码-->
    <view class="inputView"> 
      <!-- <image class="keyImage" src="/images/qq.jpg"></image>  -->
      <label class="loginLab">密碼</label> 
      <input class="inputText" password="true" placeholder="請輸入密碼" bindinput="passwordInput" /> 
    </view> 
    <!--按钮-->
    <view class="loginBtnView"> 
      <button class="loginBtn" type="primary" size="{{primarySize}}" 
          loading="{{loading}}" plain="{{plain}}" disabled="{{disabled}}" bindtap="login">登入</button> 
    </view>

  </view> 
 
</view>
 





